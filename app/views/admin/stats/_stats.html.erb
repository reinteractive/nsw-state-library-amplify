<% stats.each do |key, value| %>
<div class="card">
  <div class="card-header">
    <h3><%= display_name key %></h3>
  </div>
  <div class="card-body">
    <div class="sums">
      <% value.each do |_key, _value| %>
        <div class="sum">
          <div class="sum-label"><%= display_name _key %></div>
          <div class="sum-value"><%= value[_key].to_i %> </div>
        </div>
      <% end %>
    </div>
  </div>
</div>
<br />
<% end %>


